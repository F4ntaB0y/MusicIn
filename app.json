{
  "expo": {
    "name": "MusikIn", 
    "slug": "MusikIn", 
    "version": "1.0.0",
    "orientation": "portrait",

    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      [
        "expo-image-picker",
        {

          "photosPermission": "Izinkan $(PRODUCT_NAME) mengakses galeri foto Anda untuk memilih gambar profil."
        }
      ]
    ]


  }
}